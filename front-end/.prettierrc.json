{
  "singleQuote": true,
  "printWidth": 80,
  "tabWidth": 2,
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "^@mui/(.*)$",
    "^@core/(.*)$",
    "^@/(.*)$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "semi": true,
  "importOrderSortSpecifiers": true,
  "plugins": [
    "@trivago/prettier-plugin-sort-imports"
  ]
}
